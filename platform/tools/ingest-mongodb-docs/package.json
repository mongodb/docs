{
  "name": "ingest-mongodb-docs",
  "version": "0.12.1",
  "description": "Ingest MongoDB docs from Snooty Data API.",
  "author": "",
  "license": "Apache-2.0",
  "keywords": [],
  "packageManager": "pnpm@10.11.1",
  "engines": {
    "node": ">=18",
    "npm": ">=8"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "clean": "rm -rf build",
    "build": "tsc -b",
    "dev": "tsc -b --watch",
    "lint": "eslint ./src --ext ts --report-unused-disable-directives",
    "lint:fix": "pnpm run lint --fix && prettier ./src --check --write",
    "ingest:pages": "node ./build/cli/main.js pages update --config ./build/ingestDocs.config.js",
    "ingest:all": "node ./build/cli/main.js all --config ./build/ingestDocs.config.js",
    "test": "node --experimental-vm-modules ../../node_modules/jest/bin/jest.js --forceExit"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7",
    "@babel/types": "^7.22.5",
    "@types/common-tags": "^1.8.4",
    "@types/deep-equal": "^1.0.4",
    "@types/encodeurl": "^1.0.2",
    "@types/jest": "^29.5.2",
    "@types/node": "^12",
    "@types/node-fetch": "^2.6.13",
    "@types/yargs": "^17.0.33",
    "@typescript-eslint/eslint-plugin": "^5",
    "@typescript-eslint/parser": "^5",
    "eslint": "^8",
    "eslint-config-prettier": "^8",
    "eslint-plugin-jsdoc": "^46.4.5",
    "eslint-plugin-prettier": "^4",
    "eslint-plugin-tsdoc": "^0.2.14",
    "mongodb-memory-server": "^10.1.4",
    "nock": "^13.5.5",
    "prettier": "^2",
    "ts-jest": "catalog:",
    "typescript": "catalog:"
  },
  "dependencies": {
    "@stylistic/eslint-plugin-js": "^4.4.1",
    "@supercharge/promise-pool": "^3.2.0",
    "common-tags": "^1.8.2",
    "deep-equal": "^2.2.3",
    "docdoctor": "^0.1.2",
    "dotenv": "^16",
    "encodeurl": "^2.0.0",
    "eslint-plugin-jest": "^28.13.0",
    "jsonl-parse-stringify": "^1.0.3",
    "mongodb": "^6.16.0",
    "node-fetch": "^2.7.0",
    "winston": "^3.17.0",
    "yaml": "^2.7.1",
    "yargs": "^17.7.2"
  }
}