            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/error-response"
                }
              }
            }
          }
        },
        "tags": [
          "Query"
        ]
      },
      "get": {
        "operationId": "getQuery",
        "description": "Get a query by id",
        "parameters": [
          {
            "name": "projectId",
            "in": "path",
