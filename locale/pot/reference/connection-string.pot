#: ../source/reference/connection-string.txt:136
# 809236c9a5514551b39119c0cb80155b
msgid "Connection Options"
msgstr ""

#: ../source/reference/connection-string.txt:140
# a33b070f01af4d99b687d9d5e661180b
msgid "``true``: Initiate the connection with SSL."
msgstr ""

#: ../source/reference/connection-string.txt:142
# 486fbd96fd2640a2a515042cf286ef25
msgid "``false``: Initiate the connection without SSL."
msgstr ""

#: ../source/reference/connection-string.txt:149
# fbfad632543e4849905b7335e67ba2be
msgid "The default value is ``false``."
msgstr ""

#: ../source/reference/connection-string.txt:151
# d18caa32898b468cb50bf99859625654
