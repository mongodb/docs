msgstr ""

#: ../source/reference/operator/aggregation/listLocalSessions.txt:54
msgid "``{ }``"
msgstr ""

#: ../source/reference/operator/aggregation/listLocalSessions.txt:56
msgid "If running with access control, returns all sessions for the current authenticated user."
msgstr ""

#: ../source/reference/operator/aggregation/listLocalSessions.txt:59
msgid "If running without access control, returns all sessions."
msgstr ""

#: ../source/reference/operator/aggregation/listLocalSessions.txt:61
msgid "``{ users: [ { user: <user>, db: <db> }, ... ] }``"
msgstr ""

#: ../source/reference/operator/aggregation/listLocalSessions.txt:63
msgid "Returns all sessions for the specified users. If running with access control, the authenticated user must have privileges with :authaction:`listSession` action on the cluster to list sessions for other users."
msgstr ""

#: ../source/reference/operator/aggregation/listLocalSessions.txt:68
msgid "``{ allUsers: true }``"
msgstr ""

#: ../source/reference/operator/aggregation/listLocalSessions.txt:70
msgid "Returns all sessions for all users. If running with access control, the authenticated user must have privileges with :authaction:`listSession` action on the cluster."
msgstr ""

