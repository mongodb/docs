#: ../source/includes/apiargs/pipeline-geoNear-field.rst:7
msgid "Field"
msgstr ""

#: ../source/includes/apiargs/pipeline-geoNear-field.rst:9
msgid "Type"
msgstr ""

#: ../source/includes/apiargs/pipeline-geoNear-field.rst:11
msgid "Description"
