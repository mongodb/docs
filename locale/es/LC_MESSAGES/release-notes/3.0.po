# 997d36ad1eea4d5ab3e740d7a5146057
#: ../source/release-notes/3.0.txt:573
msgid "MongoDB Tools Enhancements"
msgstr ""

# f15741f0b5af486e8530caee49b9127c
#: ../source/release-notes/3.0.txt:575
msgid ""
"All MongoDB tools except for :program:`mongosniff` and "
":program:`mongoperf` are now written in Go and maintained as a separate "
"project."
msgstr ""

