msgid "exception 10026"
msgstr ""

#: ../source/reference/command/renameCollection.txt:102
msgid "Raised if the ``source`` namespace does not exist."
msgstr ""

#: ../source/reference/command/renameCollection.txt
msgid "exception 10027"
msgstr ""

#: ../source/reference/command/renameCollection.txt:105
msgid ""
"Raised if the ``target`` namespace exists and ``dropTarget`` is either "
"``false`` or unspecified."
msgstr ""

#: ../source/reference/command/renameCollection.txt
msgid "exception 15967"
msgstr ""

#: ../source/reference/command/renameCollection.txt:109
msgid "Raised if the ``target`` namespace is an invalid collection name."
msgstr ""

