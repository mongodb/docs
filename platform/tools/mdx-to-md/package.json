    "lint:fix": "eslint src --fix",
    "test": "vitest --run",
    "test:watch": "vitest"
  },
  "dependencies": {
    "remark": "^15.0.0",
    "remark-frontmatter": "^5.0.0",
    "remark-mdx": "^3.0.0",
    "remark-stringify": "^11.0.0",
    "unified": "^11.0.5",
    "unist-util-visit": "^5.0.0",
    "vfile": "^6.0.3"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@types/mdast": "^4.0.4",
    "@types/node": "^20.0.0",
    "@typescript-eslint/eslint-plugin": "^8.53.1",
    "@typescript-eslint/parser": "^8.53.1",
    "eslint": "^8.56.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}
