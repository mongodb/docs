#: ../source/applications/drivers.txt:31
msgid ""
"Driver version numbers use `semantic versioning <http://semver.org/>`_ or"
" \"**major.minor.patch**\" versioning system. The first number is the "
"major version, the second the minor version, and the third indicates a "
"patch."
msgstr ""

# c14d427c2a6c4be19b2c55f2e85fbb3a
#: ../source/applications/drivers.txt
msgid "Example"
msgstr ""

# 7d5f225f1d0941fa9bc51992deaf0f85
