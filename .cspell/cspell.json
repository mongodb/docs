{
  "version": "0.2",
  "language": "en",
  "dictionaries": ["mongodb-terms"],
  "dictionaryDefinitions": [
    {
      "name": "mongodb-terms",
      "path": "./.cspell/mongodb-terms.txt",
      "addWords": true
    }
  ],
  "ignorePaths": [
    "node_modules",
    ".git",
    ".next",
    ".vercel",
    "out",
    "build",
    "dist",
    "*.json",
    "*.yaml",
    "*.yml",
    "*.toml",
    "*.lock",
    "*.png",
    "*.jpg",
    "*.gif",
    "*.svg",
    "*.ico",
    "*.css",
    "*.scss",
    "**/includes/**",
    "**/public/**",
    "**/static/**"
  ],
  "ignoreRegExpList": [
    "// RST/Sphinx patterns",
    "/\\.\\.\\s+[a-z-]+::/g",
    "/:[a-z-]+:`[^`]+`/g",
    "/:ref:`[^`]+`/g",
    "/:doc:`[^`]+`/g",
    "/:manual:`[^`]+`/g",
    "/:method:`[^`]+`/g",
    "/:option:`[^`]+`/g",
    "/:param:`[^`]+`/g",
    "/:program:`[^`]+`/g",
    "/:setting:`[^`]+`/g",
    "/:binary:`[^`]+`/g",
    "/:dbcommand:`[^`]+`/g",
    "/:pipeline:`[^`]+`/g",
    "/:query:`[^`]+`/g",
    "/:update:`[^`]+`/g",
    "/:expression:`[^`]+`/g",
    "/:group:`[^`]+`/g",
    "/:projection:`[^`]+`/g",
    "/:authaction:`[^`]+`/g",
    "/:authrole:`[^`]+`/g",
    "/:data:`[^`]+`/g",
    "/:limit:`[^`]+`/g",
    "/:readmode:`[^`]+`/g",
    "/:writeconcern:`[^`]+`/g",
    "/:replstate:`[^`]+`/g",
    "/:serverstatus:`[^`]+`/g",
    "/:urioption:`[^`]+`/g",
    "/https?:\\/\\/[^\\s]+/g",
    "/\\{\\{[^}]+\\}\\}/g",
    "/\\$[a-zA-Z_][a-zA-Z0-9_]*/g",
    
    "// MDX/JSX patterns",
    "/<[A-Z][a-zA-Z]*[^>]*>/g",
    "/<\\/[A-Z][a-zA-Z]*>/g",
    "/import\\s+.*from\\s+['\"][^'\"]+['\"]/g",
    "/export\\s+(default\\s+)?/g",
    "/className=['\"][^'\"]*['\"]/g",
    "/\\{\\s*\\/\\*.*\\*\\/\\s*\\}/g",
    "/[a-f0-9]{24}/gi",
    "/[a-f0-9]{32}/gi",
    "/``[^`]+``/g",
    "/`[^`]+`/g",
    "/\\|[^|]+\\|/g",
    "/<[^>]+>/g"
  ],
  "words": [],
  "flagWords": [],
  "minWordLength": 4,
  "allowCompoundWords": true
}
