{
  "name": "mdx-to-md",
  "version": "0.1.0",
  "type": "module",
  "main": "./dist/parse.js",
  "types": "./dist/parse.d.ts",
  "exports": {
    ".": {
      "types": "./dist/parse.d.ts",
      "import": "./dist/parse.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "dev": "pnpm build && node dist/dev.js",
