{
    "type": "flexible",
    "state": "enabled",
    "development_mode_enabled": true,
    "service_name": "mongodb-atlas",
    "database_name": "todo",
    "permissions": {
        "rules": {},
        "defaultRoles": [
            {
                "name": "owner-read-write",
                "applyWhen": {},
                "read": {
                    "owner_id": "%%user.id"
                },
                "write": {
                    "owner_id": "%%user.id"
                }
            }
        ]
    },
    "queryable_fields_names": [
        "_id",
        "owner_id"
    ]
}
